<?xml version="1.0"?>
<configuration>
  <appSettings>
    <add key="LocalDemoService.Service" value="http://localhost/articles/ASPNETDesignPatterns/DemoService/Service.asmx"/>
  </appSettings>
  <connectionStrings>
    <add name="NorthwindConnectionString" connectionString="Data Source=localhost;Initial Catalog=Northwind;Integrated Security=True" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <system.web>
    <!-- add HTTP handler to perform Front Controller processing -->
    <httpModules>
      <add name="myFrontControllerModule" type="FrontController"/>
    </httpModules>
    <compilation debug="true" targetFramework="4.0"/>
    <authentication mode="Windows"/>
    <pages controlRenderingCompatibilityVersion="3.5" clientIDMode="AutoID"/>
  </system.web>
</configuration>